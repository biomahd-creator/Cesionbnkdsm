{
  "name": "@biomahd-creator/financio-design-system",
  "version": "0.2.3",
  "description": "CESIONBNK Design System - React + Tailwind CSS v4 component library for Factoring platforms",
  "private": false,
  "type": "module",
  "sideEffects": ["*.css"],
  "main": "./dist-lib/index.js",
  "module": "./dist-lib/index.js",
  "types": "./dist-lib/index.d.ts",
  "files": [
    "dist-lib",
    "theme.css",
    "styles/theme.css",
    "styles/tokens.css",
    "styles/globals.css",
    "tailwind-preset.cjs",
    "llms.txt",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist-lib/index.js",
      "types": "./dist-lib/index.d.ts"
    },
    "./ui": {
      "import": "./dist-lib/components/ui/index.js",
      "types": "./dist-lib/components/ui/index.d.ts"
    },
    "./ui/*": {
      "import": "./dist-lib/components/ui/*.js",
      "types": "./dist-lib/components/ui/*.d.ts"
    },
    "./patterns": {
      "import": "./dist-lib/components/patterns/index.js",
      "types": "./dist-lib/components/patterns/index.d.ts"
    },
    "./patterns/*": {
      "import": "./dist-lib/components/patterns/*.js",
      "types": "./dist-lib/components/patterns/*.d.ts"
    },
    "./advanced": {
      "import": "./dist-lib/components/advanced/index.js",
      "types": "./dist-lib/components/advanced/index.d.ts"
    },
    "./advanced/*": {
      "import": "./dist-lib/components/advanced/*.js",
      "types": "./dist-lib/components/advanced/*.d.ts"
    },
    "./widgets": {
      "import": "./dist-lib/components/widgets/index.js",
      "types": "./dist-lib/components/widgets/index.d.ts"
    },
    "./widgets/*": {
      "import": "./dist-lib/components/widgets/*.js",
      "types": "./dist-lib/components/widgets/*.d.ts"
    },
    "./providers": {
      "import": "./dist-lib/components/providers/index.js",
      "types": "./dist-lib/components/providers/index.d.ts"
    },
    "./hooks": {
      "import": "./dist-lib/hooks/index.js",
      "types": "./dist-lib/hooks/index.d.ts"
    },
    "./lib": {
      "import": "./dist-lib/lib/index.js",
      "types": "./dist-lib/lib/index.d.ts"
    },
    "./theme.css": "./styles/theme.css",
    "./styles.css": "./styles/globals.css",
    "./tailwind-preset": "./tailwind-preset.cjs"
  },
  "typesVersions": {
    "*": {
      "ui": ["dist-lib/components/ui/index.d.ts"],
      "ui/*": ["dist-lib/components/ui/*.d.ts"],
      "patterns": ["dist-lib/components/patterns/index.d.ts"],
      "patterns/*": ["dist-lib/components/patterns/*.d.ts"],
      "advanced": ["dist-lib/components/advanced/index.d.ts"],
      "advanced/*": ["dist-lib/components/advanced/*.d.ts"],
      "widgets": ["dist-lib/components/widgets/index.d.ts"],
      "widgets/*": ["dist-lib/components/widgets/*.d.ts"],
      "providers": ["dist-lib/components/providers/index.d.ts"],
      "hooks": ["dist-lib/hooks/index.d.ts"],
      "lib": ["dist-lib/lib/index.d.ts"]
    }
  },
  "scripts": {
    "dev": "vite",
    "dev:local": "node scripts/strip-versioned-imports.mjs && vite",
    "strip-versions": "node scripts/strip-versioned-imports.mjs",
    "flatten": "bash scripts/flatten-src.sh",
    "build": "npx tsc && vite build",
    "build:lib": "npx tsc -p tsconfig.build.json && vite build --config vite.config.lib.ts",
    "typecheck": "npx tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "check:circular": "madge --circular --extensions ts,tsx --no-spinner --ts-config tsconfig.madge.json components/",
    "check:boundary": "node scripts/check-boundary.js",
    "check:exports": "node scripts/check-exports.js",
    "check:setup": "node scripts/verify-setup.cjs",
    "validate": "npm run strip-versions && npm run typecheck && npm run test && npm run build:lib && npm run check:circular && npm run check:boundary && npm run check:exports",
    "release": "npm run validate && npm version patch && git push --follow-tags",
    "release:minor": "npm run validate && npm version minor && git push --follow-tags",
    "release:major": "npm run validate && npm version major && git push --follow-tags",
    "prepublishOnly": "npm run validate",
    "lockfile": "npm install --package-lock-only",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0",
    "tailwindcss": "^4.0.0",
    "lucide-react": ">=0.300.0"
  },
  "peerDependenciesMeta": {
    "lucide-react": {
      "optional": true
    }
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "date-fns": "^3.3.1",
    "embla-carousel-react": "^8.0.0",
    "input-otp": "^1.4.0",
    "lucide-react": "^0.344.0",
    "motion": "^11.0.0",
    "prop-types": "^15.8.1",
    "react-day-picker": "^8.10.0",
    "react-hook-form": "7.55.0",
    "react-resizable-panels": "^2.0.9",
    "recharts": "^2.12.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "^2.2.1",
    "vaul": "^1.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@tailwindcss/postcss": "^4.0.0",
    "@tanstack/react-table": "^8.13.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.0.0",
    "driver.js": "^1.3.1",
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "madge": "^7.0.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-responsive-masonry": "^2.3.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.3.3",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^3.0.0",
    "xlsx": "^0.18.5"
  },
  "keywords": [
    "design-system",
    "react",
    "tailwindcss",
    "tailwind-css-v4",
    "cesionbnk",
    "factoring",
    "ui-components",
    "ui-library",
    "component-library",
    "shadcn",
    "radix-ui",
    "typescript",
    "atomic-design",
    "dark-mode",
    "accessible",
    "wcag",
    "fintech"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/biomahd-creator/Cesionbnkdsm.git"
  },
  "homepage": "https://github.com/biomahd-creator/Cesionbnkdsm#readme",
  "bugs": {
    "url": "https://github.com/biomahd-creator/Cesionbnkdsm/issues"
  },
  "license": "MIT"
}